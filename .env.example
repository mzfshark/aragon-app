# Pinata JWT token used for pinning data on IPFS (see https://knowledge.pinata.cloud/en/articles/6191471-how-to-create-an-pinata-api-key)
NEXT_SECRET_IPFS_JWT=YOUR_VALUE_HERE

# Alchemy key to use for RPC requests (see https://www.alchemy.com/support/how-to-create-a-new-alchemy-api-key)
NEXT_SECRET_RPC_KEY=YOUR_VALUE_HERE

# Ankr key to use for RPC requests (see https://www.ankr.com/rpc/projects/)
NEXT_SECRET_ANKR_RPC_KEY=YOUR_VALUE_HERE

# dRPC key to use for RPC requests (see https://drpc.org/)
NEXT_SECRET_DRPC_RPC_KEY=YOUR_VALUE_HERE

# PEAQ key to use for RPC requests (see https://quicknode.com)
NEXT_SECRET_PEAQ_RPC_KEY=YOUR_VALUE_HERE

# Api key for connecting to the Aragon backend service
NEXT_SECRET_ARAGON_BACKEND_API_KEY=YOUR_VALUE_HERE

# Base URL do Backend utilizado pelo proxy `/api/backend/*`
# Ex.: https://api.seu-backend.tld (n√£o incluir a barra no final)
ARAGON_BACKEND_URL=YOUR_BACKEND_BASE_URL

# WalletConnect / Reown AppKit project ID (required)
# Get it from https://dashboard.reown.com
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=YOUR_VALUE_HERE

# Application environment: local | development | preview | staging | production
# Set to 'local' for local dev to relax CSP appropriately
NEXT_PUBLIC_ENV=local

# Sentry optional report endpoint for CSP violations
SENTRY_REPORT_URI=
SENTRY_REPORT_KEY=

# API allowed domain for CORS on /api/* (used by next.config.mjs)
NEXT_PUBLIC_API_ALLOWED_DOMAIN=*
